{
  "name": "janus-videoroom-client",
  "version": "3.2.7",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Hans-Peter Herzog <hherzog@sipwise.com>",
  "license": "GPL-3.0",
  "description": "",
  "keywords": [
    "janus"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sipwise/janus-client.git"
  },
  "dependencies": {
    "bluebird": "3.4.1",
    "chai": "3.5.0",
    "debug-logger": "0.4.1",
    "deepmerge": "^2.2.1",
    "lodash": "4.14.1",
    "lodash.has": "^4.5.2",
    "uuid": "2.0.2",
    "validator": "6.1.0",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tsutils": "^3.1.0",
    "typescript": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
